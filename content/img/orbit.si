  .mini-editor.slide-para p {
    line-height: 23px;
    margin: 0.6em 0;
  }
  .mars-component .btn {
    min-width: 170px !important;
    height: 40px !important;
  }
  .mars-component .slide-hldr .slide-content {
    top: 50px !important;
  }
}

/* INC2488099: Two Column Basic Row (PC9) Image Aspect Ratio Inconsistent */
.mars-component .two-column-basic-figure figure img{
    object-fit: cover;
    object-position: center;
}

